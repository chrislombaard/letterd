# Email Configuration (Optional - uses mock emails if not configured)
# Get your API key from https://resend.com/api-keys
RESEND_API_KEY=your_resend_api_key_here
# Use your verified domain from Resend (e.g., noreply@notifications.yourdomain.com)
FROM_EMAIL=noreply@notifications.yourdomain.com

# Database Configuration
DATABASE_URL="your_database_url"

# Admin Authentication (for /admin routes)
ADMIN_USER=admin
ADMIN_PASS=your_secure_password_here

# Cron Security
CRON_SECRET=your_secret_key_here